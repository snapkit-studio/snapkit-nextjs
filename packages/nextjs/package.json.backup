{
  "name": "@snapkit-studio/nextjs",
  "version": "1.6.0",
  "description": "Next.js image loader for Snapkit image optimization",
  "keywords": [
    "nextjs",
    "image",
    "loader",
    "snapkit",
    "optimization"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/snapkit-studio/snapkit-nextjs.git",
    "directory": "packages/nextjs"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "license": "MIT",
  "author": "Snapkit",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "check-types": "tsc --noEmit",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src --ext .ts",
    "test": "vitest --run",
    "test:coverage": "vitest --run --coverage",
    "version-info": "node -p \"'Package: @snapkit-studio/nextjs, Current version: ' + require('./package.json').version\""
  },
  "dependencies": {
    "@snapkit-studio/core": "workspace:*"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@snapkit-studio/react": "workspace:*",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@types/react": "^18.2.0",
    "@vitest/coverage-v8": "^2.1.8",
    "jsdom": "^23.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.0.0",
    "tsup": "8.5.0",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "next": ">=13.0.0",
    "@snapkit-studio/react": "workspace:*"
  }
}
